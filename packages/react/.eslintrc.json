{
        "env": {
                "browser": true,
                "es2021": true
        },
        "extends": [
                "plugin:react/jsx-runtime",
                "plugin:react/recommended",
                "standard"
        ],
        "parserOptions": {
                "ecmaFeatures": {
                        "jsx": true
                },
                "ecmaVersion": "latest",
                "sourceType": "module"
        },
        "plugins": [
                "react"
        ],
        "settings": {
                "react": {
                        "version": "detect"
                }
        },
        "root": true,
        "rules": {
                "indent": [
                        2,
                        8
                ],
                "semi": [
                        2,
                        "always"
                ],
                "space-before-function-paren": 0
        },
        "globals": {
                "user": true,
                "guilds": true,
                "allGuilds": true
        }
}