{
    "name": "@engel/web",
    "version": "0.1.1",
    "description": "Web server for engel",
    "private": true,
    "scripts": {
        "start": "node src/index.js",
        "dev": "concurrently -k -p \"[{pid}-{name}]\" -n \"APP,SASS,REACT\" \"npm run watch:server\" \"npm run watch:sass\" \"npm run watch:react\"",
        "watch:server": "npx nodemon --delay 2 -i public -i react -i styles -i views",
        "watch:sass": "npx sass -w styles:public/css",
        "watch:react": "npm run build --prefix react"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EngelLabs/engel-web.git"
    },
    "author": "Tinoy Thomas <tinoytpm1@gmail.com>",
    "bugs": {
        "url": "https://github.com/EngelLabs/engel-web/issues"
    },
    "homepage": "https://github.com/timbot-labs/engel-web#readme",
    "dependencies": {
        "@timbot/logger": "github:timbot-labs/timbot-logger",
        "bluebird": "^3.7.2",
        "connect-redis": "^6.0.0",
        "dotenv": "^10.0.0",
        "eris": "^0.16.1",
        "express": "^4.17.1",
        "express-handlebars": "^6.0.2",
        "express-session": "^1.17.2",
        "getenv": "^1.0.0",
        "ioredis": "^4.28.1",
        "mongodb": "^4.2.1",
        "react-router": "^6.2.1",
        "superagent": "^7.1.1",
        "uuid": "^7.0.3",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/plugin-transform-runtime": "^7.16.4",
        "babel-loader": "^8.2.3",
        "concurrently": "^6.4.0",
        "nodemon": "^2.0.15",
        "webpack": "^5.64.4",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.6.0"
    }
}
