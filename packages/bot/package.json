{
    "name": "@engel/bot",
    "version": "0.1.2",
    "private": true,
    "description": "A Discord bot to provide some nice utilities.",
    "author": "Tinoy Thomas <tinoytpm1@gmail.com>",
    "scripts": {
        "prestart": "npm run build && npm run config",
        "start": "node build/src/index.js",
        "dev": "concurrently -i -k -p \"[{pid}-{name}]\" -n \"APP,TS\" \"npm run watch\" \"npm run watch:ts\"",
        "watch": "npx nodemon --delay 3 -i build/src/modules/",
        "watch:ts": "npx tsc -w --noEmit -p tsconfig.json",
        "config": "node scripts/config.js",
        "build": "npx tsc -p tsconfig.json",
        "lint": "npx eslint \"src/**/*\"",
        "pm2": "npx pm2 start npm --name engel-bot -- start",
        "pm2:dev": "npx pm2 start npm --name engel-bot-dev -- run dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/EngelLabs/engel-bot.git"
    },
    "bugs": {
        "url": "https://github.com/EngelLabs/engel-bot/issues"
    },
    "homepage": "https://github.com/EngelLabs/engel-bot#readme",
    "dependencies": {
        "@engel/core": "0.2.0",
        "bufferutil": "^4.0.5",
        "eris": "github:abalabahaha/eris#dev",
        "erlpack": "github:abalabahaha/erlpack",
        "eventemitter3": "^4.0.7",
        "moment": "^2.29.1",
        "pako": "^1.0.11",
        "pretty-ms": "^7.0.1",
        "require-reload": "^0.2.2",
        "superagent": "^6.1.0",
        "typescript": "4.5",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "@types/superagent": "^4.1.15",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "concurrently": "^7.0.0",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.2.0",
        "mongoose": "^6.2.3",
        "nodemon": "^2.0.15"
    }
}