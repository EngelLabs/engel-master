{
    "compilerOptions": {
        "outDir": "./build",
        "module": "CommonJS",
        "target": "ESNext",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true,
        "noImplicitAny": true,
        "preserveWatchOutput": true,
        "removeComments": true,
        "sourceMap": true
},
    "include": [
        "./src/**/*",
    ],
}