{
    "name": "engel",
    "version": "0.1.2",
    "private": true,
    "description": "A Discord bot to provide some nice utilities.",
    "scripts": {
        "start": "node build/index.js",
        "dev": "concurrently -k -p \"[{pid}-{name}]\" -n \"APP,TS\" \"npm run watch\" \"npm run watch:ts\"",
        "watch": "npx nodemon --delay 3 -i build/modules/",
        "watch:ts": "npx tsc -w",
        "config": "node scripts/config.js"
    },
    "author": "Tinoy Thomas <tinoytpm1@gmail.com>",
    "dependencies": {
        "@engel/core": "github:engel-labs/engel-core#master",
        "bluebird": "^3.7.2",
        "bufferutil": "^4.0.5",
        "eris": "github:abalabahaha/eris#dev",
        "erlpack": "github:abalabahaha/erlpack",
        "eventemitter3": "^4.0.7",
        "moment": "^2.29.1",
        "pako": "^1.0.11",
        "pretty-ms": "^7.0.1",
        "require-reload": "^0.2.2",
        "superagent": "^6.1.0",
        "typescript": "^4.5.5",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "concurrently": "^7.0.0",
        "mongoose": "^6.2.3",
        "nodemon": "^2.0.15"
    }
}
