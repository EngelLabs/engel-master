{
    "name": "engel-master",
    "version": "0.1.0",
    "private": true,
    "description": "Master repository for engel",
    "author": "Tinoy Thomas <tinoytpm1@gmail.com>",
    "scripts": {
        "postinstall": "npm run build",
        "bot": "npm start -w packages/bot",
        "web": "npm start -w packages/web",
        "pm2": "npm run pm2:bot & npm run pm2:web",
        "pm2:dev": "npm run pm2:bot:dev & npm run pm2:web:dev",
        "lint": "npm run lint:core & npm run lint:utils & npm run lint:bot & npm run lint:web",
        "build": "npm run build:core && npm run build:utils",
        "postbuild": "npm run build:bot & npm run build:web",
        "dev": "npm run dev:core & npm run dev:bot & npm run dev:web",
        "pm2:bot": "npm run pm2 -w packages/bot",
        "pm2:web": "npm run pm2 -w packages/web",
        "pm2:bot:dev": "npm run pm2:dev -w packages/bot",
        "pm2:web:dev": "npm run pm2:dev -w packages/web",
        "lint:bot": "npm run lint -w packages/bot",
        "lint:web": "npm run lint -w packages/web",
        "lint:core": "npm run lint -w packages/core",
        "lint:utils": "npm run lint -w packages/utils",
        "build:bot": "npm run build -w packages/bot",
        "build:web": "npm run build -w packages/web",
        "build:core": "npm run build -w packages/core",
        "build:utils": "npm run build -w packages/utils",
        "dev:bot": "npm run dev -w packages/bot",
        "dev:web": "npm run dev -w packages/web",
        "dev:core": "npm run dev -w packages/core",
        "config": "npm run config -w packages/bot"
    },
    "workspaces": [
        "packages/*"
    ]
}